{
  "framework": "vite",
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps",
  "routes": [
    {
      "src": "/post/(.*)",
      "dest": "/index.html" 
    },
    {
      "src": "/blog_posts/(.*)",
      "dest": "/blog_posts/$1" 
    },
    {
      "src": "/(.*)",
      "dest": "/index.html" 
    }
  ]
}
